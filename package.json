{
    "name": "cursor-language-patch",
    "description": "A tool to patch the cursor language registry for Bun.",
    "private": true,
    "main": "src/index.ts",
    "version": "0.0.2",
    "bin": {
        "cursor-language-patch": "./src/index.ts"
    },
    "scripts": {
        "patch": "bun run ./src/index.ts --patch",
        "restore": "bun run ./src/index.ts --restore",
        "help": "bun run ./src/index.ts help"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/os-locale": "^4.0.2",
        "@types/win-version-info": "^3.1.3",
        "eslint-config-retn0": "^2.4.0"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "acorn": "^8.14.1",
        "acorn-walk": "^8.3.4",
        "ora": "^8.2.0",
        "os-locale": "^6.0.2",
        "regedit": "^5.1.4",
        "win-version-info": "^6.0.1"
    }
}